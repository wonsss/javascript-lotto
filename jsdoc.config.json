{
  "plugins": ["plugins/markdown"],
  "opts": {
    "readme": "./docs/README.md"
  },
  "recurseDepth": 10,
  "source": {
    "include": [
      ".",
      "./src",
      "./src/js",
      "./src/js/controller",
      "./src/js/model",
      "./src/js/view"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
